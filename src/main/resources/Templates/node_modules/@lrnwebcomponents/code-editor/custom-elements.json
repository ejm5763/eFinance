    {
      "name": "monaco-element",
      "description": "`monaco-element`\nWebcomponent wrapper for the monaco editor.\n\nSets value, language and theme.\nOffers a value-changed event.\n\nPartly influenced by https://github.com/PolymerVis/monaco-editor",
      "attributes": [
        {
          "name": "unique-key",
          "description": "THIS MAKES MULTIPLES EDITORS WORK BECAUSE OF EVENTS\nDO NOT MESS WITH THIS AND IT HAS TO BE SET",
          "type": "string"
        },
        {
          "name": "value",
          "type": "string",
          "default": "\"\""
        },
        {
          "name": "font-size",
          "type": "number",
          "default": "16"
        },
        {
          "name": "read-only",
          "type": "boolean",
          "default": "false"
        },
        {
          "name": "eventTypes",
          "type": "object",
          "default": "{\"ready\":\"ready\",\"focus\":\"focus\",\"blur\":\"blur\",\"valueChanged\":\"valueChanged\",\"languageChanged\":\"languageChanged\",\"themeChanged\":\"themeChanged\"}"
        },
        {
          "name": "language",
          "type": "string",
          "default": "\"javascript\""
        },
        {
          "name": "theme",
          "type": "string",
          "default": "\"vs-dark\""
        },
        {
          "name": "lib-path",
          "type": "string",
          "default": "\"node_modules/monaco-editor/min/vs\""
        },
        {
          "name": "autofocus",
          "description": "automatically set focus on the iframe",
          "type": "boolean",
          "default": "false"
        },
        {
          "name": "hide-line-numbers",
          "description": "hide line numbers",
          "type": "boolean",
          "default": "false"
        },
        {
          "name": "editor-reference",
          "type": "string"
        }
      ],
